<?xml version="1.0" encoding="UTF-8"?>
<?use version="3.0.0"?>
<plugin name="XMIHandler" version="0.1" publisher="Emil Huseynli">
    <actions>
      <action label="Export to XMI" icon="resources/export.png"
      class="org.tzi.use.plugins.xmihandler.gui.ActionExportXMI" tooltip="Export to XMI"
      menu="View" menuitem="Export to XMI" toolbaritem="plugins"
      id="org.tzi.use.plugins.xmihandler.gui.ActionExportXMI">
    </action>    
      <action label="Import from XMI" icon="resources/import.png"
      class="org.tzi.use.plugins.xmihandler.gui.ActionImportXMI" tooltip="Import from XMI"
      menu="View" menuitem="Import from XMI" toolbaritem="plugins"
      id="org.tzi.use.plugins.xmihandler.gui.ActionImportXMI">
    </action>    
    </actions>
	<commands>
	   <command class="org.tzi.use.plugins.xmihandler.cmd.XMIHandlerCmd"
      id="org.tzi.use.xmihandler.cmd.XMIHandlerCmd" shellcmd="export xmi"
      label="Export to XMI" help="Exports USE Model to XMI File">
    </command>
     <command class="org.tzi.use.plugins.xmihandler.cmd.XMIHandlerCmd"
      id="org.tzi.use.xmihandler.cmd.XMIHandlerCmd" shellcmd="import xmi"
      label="Import from XMI" help="Imports UML Model from XMI File">
    </command>
	</commands>
</plugin>
